package com.cx;

public abstract class Animal {
    private int name;
    private final String city="ä¸Šæµ·";

    //æˆå‘˜æ–¹æ³•
    public void show(){
        name=1;
//        city="åŒ—äº¬"ï¼›
    }



    //æŠ½è±¡æ–¹æ³•
    public abstract void eat();
    public void sleep(){
        System.out.println("ç¡è§‰");
    }
}



--------


package com.cx;

public class AnimalDemo {
    public static void main(String[] args) {
        Animal a=new Cat();
        a.eat();
        a.sleep();

    }
}



