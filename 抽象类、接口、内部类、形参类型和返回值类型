package com.jk;

public interface Jumpping {
    void eat();
}


------


package com.jk;

public class Animal {
    public void show(){
        System.out.println("åŠ¨ç‰©");
    }
}



------


package com.jk;

public class Cat extends Animal implements Jumpping {

    @Override
    public void eat() {
        System.out.println("çŒ«åƒé±¼");
    }
    public void show() {
        System.out.println("é‡å†™åŠ¨ç‰©");
    }
    }



------


package com.jk;

public class JumppingDemo {
    public static void main(String[] args) {
        Jumpping J=new Cat();
        J.eat();
        Animal a=new Cat();
        a.show();
    }

}



