8ã€æ•°ç»„åè½¬
public class Text03 {
    public static void main(String[] args) {
        int[] arr={11,22,33,44,55};
        for (int start=0,end=arr.length-1;start<=end;start++,end--){     //â€»â€»â€»
            int temp;
            temp =arr[start];
            arr[start]=arr[end];
            arr[end]=temp;
        }
        for (int x=0;x<arr.length;x++){
            System.out.println(arr[x]);
        }
    }
}

