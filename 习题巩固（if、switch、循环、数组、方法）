2ã€å‡è‚¥è®¡åˆ’switchç‰ˆ
import java.util.Scanner;
public class xuexi {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.println("è¯·è¾“å…¥æ˜ŸæœŸæ•°");
        int a=sc.nextInt();
     switch (a){
         case 1:
             System.out.println("ç¯®çƒ");
             break;
         case 2:
             System.out.println("è·³ç»³");
             break;
         case 3:
             System.out.println("è·‘æ­¥");
             break;
         default:
             System.out.println("è¾“å…¥çš„æ˜ŸæœŸæ•°æœ‰è¯¯");
             break;
     }
    }
}

