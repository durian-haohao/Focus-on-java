public class xuexi {
    public static void main(String[] args) {
        int z;
        for (int x=0;x<=20;x++){
            for (int y=0;y<=33;y++){
                z=100-x-y;
                if(5*x+3*y+z/3==100&&z%3==0){
                    System.out.println(x+","+y+","+z);
                }
            }
        }
    }
}
å¯ä»¥å†™æˆï¼š
public class xuexi {
    public static void main(String[] args) {
        for (int x=0;x<=20;x++){
            for (int y=0;y<=33;y++){
                for (int z=0;z<=100;z++){
                    if(5*x+3*y+z/3==100&&x+y+z==100&&z%3==0){
                        System.out.println(x+","+y+","+z);
                    }
                }
            }
        }
    }
}

